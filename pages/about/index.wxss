.container{
    position: absolute;
    background-color: #ffffff;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.top{
    width: 100%;
    height: 300rpx;
    background-color: #f1bc0f;
}

.nav{
    display: flex;
    flex-direction: row;
    height: 200rpx;
}

.head{
    display: flex;
    margin: 40rpx 40rpx 0 100rpx;
    width:140rpx;
    height:140rpx;
    justify-content: center;
}

.nav-1{
    margin-top: 80rpx;
    display: flex;
    justify-content: center;
    font-size: 35rpx;
    color: white;
}

.middle{
    width: 100%;
    height: 200rpx;
    background-color: #f7f7f7;
}

.bottom{
    width: 100%;
    height: 552rpx;
    display: flex;
    flex-direction: column;

}

.bottom-line{
    display: flex;
    width: 100%;
    height: 92rpx;
    background-color: white;
    margin-top: 1rpx;
    flex-direction: row;
    justify-content: space-between;
}

.bottom-left{
    display: flex;
    flex-direction: row;
}

.bottom-image{
    margin-left: 20rpx;
    margin-top: 21rpx;
    width: 50rpx;
    height: 50rpx;
}

.bottom-context{
    margin-top: 20rpx;
    margin-left: 30rpx;
    height: 50rpx;
    font-size: 35rpx;
}

.arrow{
    margin-top: 20rpx;
    font-size: 45rpx;
    width: 80rpx;
    height: 50rpx;
    text-align: center;
}

.item{
    position: absolute;
    width: 710rpx;
    margin-left: 20rpx;
    margin-top: 200rpx;
    height: 270rpx;
    border-radius: 10rpx;
    z-index: 2;
    background-color: white;
}

.item-all{
    padding: 40rpx;
}

.item-1{
    display: flex;
    justify-content: space-between;
    height: 80rpx;
}

.item-11{
    font-size: 40rpx;
    font-weight: 600;
    text-align: center;
}

.item-12{
    font-size: 30rpx;
    text-align: center;
}

.line{
    background: #f8f8f8;
    width: 100%;
    height: 5rpx;
}

.item-2{
    display: flex;
    flex-direction: row;
}

.item-21{
    margin-top: 20rpx;
    width: 160rpx;
    text-align: center;
}

.image{
    width: 70rpx;
    height: 70rpx;
}